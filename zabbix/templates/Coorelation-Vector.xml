<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>7.0</version>
    <template_groups>
        <template_group>
            <uuid>e92dcdae80c7452b80d40bca86a75990</uuid>
            <name>test</name>
        </template_group>
    </template_groups>
    <templates>
        <template>
            <uuid>39ba6ff516d848de81973b3adee5de58</uuid>
            <template>Correlation-Vector</template>
            <name>Correlation-Vector</name>
            <groups>
                <group>
                    <name>test</name>
                </group>
            </groups>
            <items>
                <item>
                    <uuid>faf50fbd766141c09fe4bcbe9fb7189b</uuid>
                    <name>Application Status</name>
                    <type>CALCULATED</type>
                    <key>application.status</key>
                    <value_type>FLOAT</value_type>
                    <params>last(//database.status)*last(//server.problem)*last(//webserver.status)*last(//website.status)</params>
                    <tags>
                        <tag>
                            <tag>component</tag>
                            <value>correlation</value>
                        </tag>
                    </tags>
                </item>
                <item>
                    <uuid>978c3a2f1cd946f8b6b7e33bf87315d4</uuid>
                    <name>Database Status</name>
                    <type>DEPENDENT</type>
                    <key>database.status</key>
                    <delay>0</delay>
                    <value_type>FLOAT</value_type>
                    <preprocessing>
                        <step>
                            <type>JSONPATH</type>
                            <parameters>
                                <parameter>$.database.status</parameter>
                            </parameters>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>vu_problem_correlation.py[{$DATABASE.STATUS},{$SERVER.STATUS},{$WEBSERVER.STATUS},{$WEBSITE.STATUS},{$API},{$HOST.URL},{HOST.NAME},{$DATABASE},{$SERVER},{$WEBSERVER}]</key>
                    </master_item>
                    <tags>
                        <tag>
                            <tag>component</tag>
                            <value>correlation</value>
                        </tag>
                    </tags>
                </item>
