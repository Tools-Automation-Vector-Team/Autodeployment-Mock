            </items>
            <discovery_rules>
                <discovery_rule>
                    <uuid>850be858813d410d8cb17c8c5e727577</uuid>
                    <name>Corelated Items</name>
                    <type>DEPENDENT</type>
                    <key>correlated.host</key>
                    <delay>0</delay>
                    <lifetime>1d</lifetime>
                    <item_prototypes>
                        <item_prototype>
                            <uuid>525d5ed5ea944bc48d513f5a0cc0925d</uuid>
                            <name>{#TYPE} status</name>
                            <type>DEPENDENT</type>
                            <key>status[{#TYPE}]</key>
                            <delay>0</delay>
                            <value_type>FLOAT</value_type>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <parameters>
                                        <parameter>$.data[?(@.type=='{#TYPE}')].status</parameter>
                                    </parameters>
                                </step>
                                <step>
                                    <type>JSONPATH</type>
                                    <parameters>
                                        <parameter>$.[0]</parameter>
                                    </parameters>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>vu_problem_correlation.py[{$DATABASE.STATUS},{$SERVER.STATUS},{$WEBSERVER.STATUS},{$WEBSITE.STATUS},{$API},{$HOST.URL},{HOST.NAME},{$DATABASE},{$SERVER},{$WEBSERVER}]</key>
                            </master_item>
                            <tags>
                                <tag>
                                    <tag>component</tag>
                                    <value>correlation</value>
                                </tag>
                            </tags>
                        </item_prototype>
                    </item_prototypes>
                    <master_item>
                        <key>vu_problem_correlation.py[{$DATABASE.STATUS},{$SERVER.STATUS},{$WEBSERVER.STATUS},{$WEBSITE.STATUS},{$API},{$HOST.URL},{HOST.NAME},{$DATABASE},{$SERVER},{$WEBSERVER}]</key>
                    </master_item>
                    <lld_macro_paths>
                        <lld_macro_path>
                            <lld_macro>{#COUNT}</lld_macro>
                            <path>$.count</path>
                        </lld_macro_path>
                        <lld_macro_path>
                            <lld_macro>{#STATUS}</lld_macro>
                            <path>$.status</path>
                        </lld_macro_path>
                        <lld_macro_path>
                            <lld_macro>{#TYPE}</lld_macro>
                            <path>$.type</path>
                        </lld_macro_path>
                    </lld_macro_paths>
                    <preprocessing>
                        <step>
                            <type>JSONPATH</type>
                            <parameters>
                                <parameter>$.data[*]</parameter>
                            </parameters>
                        </step>
                    </preprocessing>
                </discovery_rule>
            </discovery_rules>
            <tags>
                <tag>
                    <tag>component</tag>
                    <value>correlation</value>
                </tag>
            </tags>
            <macros>
                <macro>
                    <macro>{$DATABASE}</macro>
                </macro>
                <macro>
                    <macro>{$DATABASE.STATUS}</macro>
                    <value>1</value>
                    <description>Set &quot;1&quot; if the database is being monitored, and &quot;0&quot; if it is not. This flag indicates the current status of database monitoring.</description>
                </macro>
                <macro>
                    <macro>{$SERVER}</macro>
                </macro>
                <macro>
                    <macro>{$SERVER.STATUS}</macro>
                    <value>1</value>
                    <description>Set &quot;1&quot; if the server is being monitored, and &quot;0&quot; if it is not. This flag indicates the current status of server monitoring.</description>
                </macro>
                <macro>
                    <macro>{$WEBSERVER}</macro>
                </macro>
                <macro>
                    <macro>{$WEBSERVER.STATUS}</macro>
                    <value>1</value>
                    <description>Set &quot;1&quot; if the web server is being monitored, and &quot;0&quot; if it is not. This flag indicates the current status of web server monitoring.</description>
                </macro>
                <macro>
                    <macro>{$WEBSITE.STATUS}</macro>
                    <value>1</value>
                    <description>Set &quot;1&quot; if the website is being monitored, and &quot;0&quot; if it is not. This flag indicates the current status of website monitoring.</description>
                </macro>
            </macros>
        </template>
    </templates>
</zabbix_export>
